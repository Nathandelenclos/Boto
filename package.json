{
  "name": "valorat",
  "version": "1.0.0",
  "main": "src/main.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "ts-node --project tsconfig.json src/main.ts",
    "dev": "ts-node --project tsconfig.json src/main.ts --watch",
    "test:lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --cache --fix",
    "test:types": "tsc --noEmit",
    "test:unit": "jest --passWithNoTests",
    "test": "yarn test:lint && yarn test:types && yarn test:unit"
  },
  "dependencies": {
    "@types/lodash": "^4.14.202",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "lodash": "^4.17.21",
    "rcon-client": "^4.2.4",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "ts-jest": "^29.1.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.0"
  }
}
